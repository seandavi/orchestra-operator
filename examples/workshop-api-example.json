{
  "name": "data-science-workshop",
  "duration": "4h",
  "image": "bioconductor/bioconductor_docker:devel",
  "resources": {
    "cpu": "1",
    "memory": "2Gi",
    "cpuRequest": "500m",
    "memoryRequest": "1Gi"
  },
  "storage": {
    "size": "10Gi"
  },
  "ingress": {
    "host": "workshop.local",
    "annotations": {
      "kubernetes.io/ingress.class": "traefik"
    }
  }
}
